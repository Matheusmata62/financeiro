{
  "buildCommand": "pip install -r requirements.txt",
  "devCommand": "streamlit run src/dashboard.py",
  "env": {
    "PYTHONUNBUFFERED": "1",
    "STREAMLIT_SERVER_HEADLESS": "true",
    "STREAMLIT_SERVER_PORT": "3000",
    "STREAMLIT_SERVER_ENABLE_CORS": "false"
  },
  "functions": {
    "src/dashboard.py": {
      "runtime": "python3.9"
    }
  }
}
